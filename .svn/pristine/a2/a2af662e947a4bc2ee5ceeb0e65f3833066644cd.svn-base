.boards-title-left {
    margin-left: 22px;
    line-height: 30px;
    .title-bread {
        position: relative;
        .path {
            padding-left: 1rem;
            padding-right: 0.5em;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
            width: 13rem;
            direction: rtl;
            text-align: right;
            line-height: 30px;
            background: url('../../../static/img/type-space.png') no-repeat center left;
            cursor: pointer;
            /* &.pathicon100 {
                background: url('../../../static/img/type-sprint.png') no-repeat center left;
            }
            &.pathiconpast100 {
                background: url('../../../static/img/sprint_past.gif') no-repeat center left;
            }
            &.pathicon2002 {
                background: url('../../../static/img/tracking-open.gif') no-repeat center left;
            }
            &.pathicon99 {
                background: url('../../../static/img/type-scrum.png') no-repeat center left;
            }
            &.pathiconpast99 {
                background: url('../../../static/img/scrum_past.png') no-repeat center left;
            }
            &.pathicon98 {
                padding-left: 1rem;
                background: url('../../../static/img/type-space.png') no-repeat center left;
            }
            &.pathiconpast98 {
                padding-left: 1rem;
                background: url('../../../static/img/type-space.png') no-repeat center left;
            }
            &.pathicon33 {
                padding-left: 1rem;
                background: url('../../../static/img/icon_backlogfolder.png') no-repeat center left;
            }
            &.pathicon0 {
                padding-left: 1rem;
                background: url('../../../static/img/type-root-normal.png') no-repeat center left;
            }
            // &.pathicon100 {
            //     background: url('../../../static/img/milestone.png') no-repeat left center;
            //     padding-left: 18px;
            // }*/
            .title-common {
                //icon
                .path-text {
                    color: red;
                }
                .breadProject {
                    padding-left: 1rem;
                    &.secondIcon100 {
                        background: url('../../../static/img/type-sprint.png') no-repeat center left;
                    }
                    &.secondIcon2002 {
                        background: url('../../../static/img/tracking-open.gif') no-repeat center left;
                    }
                    &.secondIcon99 {
                        background: url('../../../static/img/type-scrum.png') no-repeat center left;
                    }
                }
                .thirdLevelContent {
                    margin-left: 0.5rem;
                    padding-left: 1rem;
                    &.thirdIcon100 {
                        background: url('../../../static/img/type-sprint.png') no-repeat center left;
                    }
                    &.thirdIcon2002 {
                        background: url('../../../static/img/tracking-open.gif') no-repeat center left;
                    }
                    &.thirdIcon99 {
                        background: url('../../../static/img/type-scrum.png') no-repeat center left;
                    }
                }
            }
        }
        .search-mes {
            margin-left: 1rem;
            font-size: 24px;
            transform: rotate(90deg);
            cursor: pointer;
            &:hover {
                color: #58BFA8;
            }
        }
        .transform {
            -webkit-transform: rotate(180deg);
            transform: rotate(180deg);
            transition: transform .1s linear;
        }
        .search-mes-panel {
            position: absolute;
            top: 30px;
            left: 50%;
            width: 450px;
            max-height: 500px;
            overflow-y: auto;
            overflow-x: hidden;
            padding-bottom: 10px;
            background: #fff;
            z-index: 100;
            .search-mes-panel-top {
                width: 448px;
                height: 30px;
                line-height: 30px;
                text-align: right;
                background: #F5F5F5;
                border-bottom: 1px solid #ccc;
                .x {
                    display: block;
                    width: 20px;
                    height: 20px;
                    line-height: 20px;
                    text-align: center;
                    margin: 3px;
                    border-radius: 2px;
                    cursor: pointer;
                    &:hover {
                        border: 1px solid #999999;
                        background: rgb(235, 235, 235);
                    }
                }
            }
            .search-mes-input {
                width: 400px;
                height: 34px;
                border: 1px solid #ccc;
                border-radius: 3px;
                margin: 10px 20px;
                .search-msg-icon {
                    display: block;
                    width: 34px;
                    height: 32px;
                    line-height: 34px;
                    text-align: center;
                    background: #F5F5F5;
                    font-size: 22px;
                    cursor: pointer;
                    &:hover {
                        background: #eee;
                    }
                }
                .search-mes-input-bar {
                    width: 364px;
                    border: none;
                    outline: none;
                }
            }
            .mes-panel-list {
                margin: 10px 10px;
                .panel-first-title {
                    cursor: default;
                    .first-title-icon {
                        height: 16px;
                        line-height: 16px;
                        i {
                            font-size: 20px;
                        }
                        cursor: pointer;
                        &.firstIconRotate {
                            transform: rotate(-45deg);
                        }
                    }
                    .first-title-content {
                        display: block;
                        height: 16px;
                        line-height: 16px;
                        padding: 2px 22px;
                        margin-left: 20px;
                        background: url('../../../static/img/type-root-normal.png') no-repeat left center;
                    }
                    .second-title {
                        margin-left: 20px;
                        .dev-items-icon {
                            height: 16px;
                            line-height: 16px;
                            &.rotate {
                                transform: rotate(-90deg);
                            }
                            i {
                                font-size: 20px;
                            }
                            cursor: pointer;
                        }
                        .sprintCaret {
                            position: absolute;
                            top: 0;
                            left: 0;
                        }
                        .second-title-content {
                            display: block;
                            height: 16px;
                            width: 360px;
                            line-height: 16px;
                            padding: 2px 18px;
                            margin-left: 10px;
                            overflow: hidden;
                            text-overflow: ellipsis;
                            white-space: nowrap;
                            background: url('../../../static/img/type-space.png') no-repeat left center;
                            user-select: none;
                            cursor: pointer;
                        }
                        .dev-items {
                            margin-top: 5px;
                        }
                    }
                    .dev-item {
                        margin-left: 15px;
                        li {
                            width: 360px;
                            overflow: hidden;
                            text-overflow: ellipsis;
                            white-space: nowrap;
                        }
                        .sub-item {
                            position: relative;
                            margin-left: 0.5rem;
                            line-height: 25px;
                            .dev-items-icon {
                                &.rotate {
                                    margin-top: 2px;
                                    transform: rotate(-90deg);
                                }
                            }
                            .sub-item-text {
                                padding-left: 1rem;
                                margin-left: 1rem;
                                width: 15rem;
                                overflow: hidden;
                                text-overflow: ellipsis;
                                white-space: nowrap;
                                cursor: pointer;
                                &.icon99 {
                                    background: url('../../../static/img/type-scrum.png') no-repeat center left;
                                }
                                &.icon100 {
                                    background: url('../../../static/img/type-sprint.png') no-repeat center left;
                                }
                                &.icon2002 {
                                    background: url('../../../static/img/tracking-open.gif') no-repeat center left;
                                }
                            }
                            .sprint-icon {
                                margin-left: 15px;
                            }
                            .sprint-list {
                                margin-left: 1.4rem;
                                margin-top: -10px;
                                .sprint-list-item {
                                    margin-top: 5px;
                                    .sprint-list-text {
                                        display: block;
                                        height: 1rem;
                                        width: 10rem;
                                        line-height: 1rem;
                                        padding: 2px 18px;
                                        margin-left: 10px;
                                        overflow: hidden;
                                        text-overflow: ellipsis;
                                        white-space: nowrap;
                                        background: url('../../../static/img/sprint_past.gif') no-repeat left center;
                                        user-select: none;
                                        cursor: pointer;
                                    }
                                }
                            }
                            .task-list {
                                margin-left: 2rem;
                                .task-list-item {
                                    position: relative;
                                    .folder-items {
                                        position: absolute;
                                        top: 0;
                                        left: 0;
                                        i {
                                            font-size: 20px;
                                        }
                                    }
                                    .folderName {
                                        margin-left: 1rem;
                                        padding-left: 1.2rem;
                                        cursor: pointer;
                                        &.folder0 {
                                            background: url('../../../static/img/type-root-normal.png') no-repeat center left;
                                        }
                                        &.folder33 {
                                            background: url('../../../static/img/icon_backlogfolder.png') no-repeat center left;
                                        }
                                        &.folder99 {
                                            background: url('../../../static/img/type-scrum.png') no-repeat center left;
                                        }
                                        &.folder100 {
                                            background: url('../../../static/img/type-sprint.png') no-repeat center left;
                                        }
                                    }
                                    .folders {
                                        margin-left: 2rem;
                                        .folder-item {
                                            padding-left: 1.2rem;
                                            cursor: pointer;
                                            &.file0 {
                                                background: url('../../../static/img/type-root-normal.png') no-repeat center left;
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
            &.searchPanel-enter-active {
                animation: searchPanel-in .3s;
            }
            &.searchPanel-leave-active {
                animation: searchPanel-out .2s;
            }
            @keyframes searchPanel-in {
                0% {
                    transform: scale(0.7);
                }
                100% {
                    transform: scale(1);
                }
            }
            @keyframes searchPanel-out {
                0% {
                    transform: scale(1);
                }
                100% {
                    transform: scale(0.7);
                    opacity: 0;
                }
            }
            &::-webkit-scrollbar-track {
                border-radius: 10px;
                background-color: #F5F5F5;
            }
            &::-webkit-scrollbar {
                width: 8px;
                height: 8px;
            }
            &::-webkit-scrollbar-thumb {
                border-radius: 25px;
                -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, .8);
            }
            &::-webkit-scrollbar-track:vertical {
                background-color: transparent;
            }
        }
        .filter {
            position: relative;
            margin-left: 0.5rem;
            //position: absolute;
            font-size: 18px;
            cursor: pointer;
            i.fa-filter:hover {
                color: #58BFA8;
            }
            .filter-select {
                transform: scale(0.7);
                position: absolute;
                top: 30px;
                left: -100px;
                width: 200px;
                height: 234px;
                background: #fff;
                z-index: 10;
                &.filterPanelShow {
                    transform: scale(1);
                    transition: transform 5s linear;
                }
                .filter-top {
                    width: 198px;
                    height: 30px;
                    line-height: 30px;
                    text-align: right;
                    background: #F5F5F5;
                    border-bottom: 1px solid #ccc;
                    .x {
                        display: block;
                        width: 20px;
                        height: 20px;
                        line-height: 20px;
                        text-align: center;
                        margin: 3px;
                        border-radius: 2px;
                        cursor: pointer;
                        font-size: 14px;
                        font-weight: 100;
                        &:hover {
                            border: 1px solid #999999;
                            background: rgb(235, 235, 235);
                        }
                    }
                    .filter-top-text {
                        padding-left: 10px;
                        font-size: 14px;
                    }
                }
                .filter-by {
                    //padding: 10px 5px;
                    .filter-by-item {
                        border-bottom: 1px solid #ccc;
                        padding-left: 10px;
                        margin: 0 5px;
                        &:last-child {
                            border-bottom: none;
                        }
                        a {
                            display: block;
                            margin: 0 5px;
                            padding: 5px 0 35px 40px;
                            font-size: 14px;
                            color: #1F2532;
                            &.followby {
                                height: 24px;
                                background: url('../../../static/img/followby.png') no-repeat left center;
                            }
                            &.assigned {
                                height: 24px;
                                background: url('../../../static/img/assignedby.png') no-repeat left center;
                            }
                            &.duedate {
                                height: 24px;
                                background: url('../../../static/img/duedate.png') no-repeat left center;
                            }
                            &.tags {
                                height: 24px;
                                background: url('../../../static/img/tags.png') no-repeat left center;
                            }
                            &.statusby {
                                height: 24px;
                                background: url('../../../static/img/statusby.png') no-repeat left center;
                            }
                        }
                    }
                }
                &.filterPanel-enter-active {
                    animation: animation-in .3s;
                }
                &.filterPanel-leave-active {
                    animation: animation-out .2s;
                }
                @keyframes animation-in {
                    0% {
                        transform: scale(0.7);
                    }
                    100% {
                        transform: scale(1);
                    }
                }
                @keyframes animation-out {
                    0% {
                        transform: scale(1);
                    }
                    100% {
                        transform: scale(0.7);
                        opacity: 0;
                    }
                }
            }
        }
    }
}

.setting-btn {
    margin-left: 10px;
    cursor: pointer;
    .setting-btn {
        display: inline-block;
        padding: 1px 10px;
        border: 1px solid #ccc;
        &:hover {
            background: #86d9e4;
            color: white;
            cursor: pointer;
        }
    }
}

.boardsBackend {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: #000;
    opacity: .5;
    z-index: 9;
}

.demo-spin-icon-load {
    animation: ani-demo-spin 1s linear infinite;
}

@keyframes ani-demo-spin {
    from {
        transform: rotate(0deg);
    }
    50% {
        transform: rotate(180deg);
    }
    to {
        transform: rotate(360deg);
    }
}

.demo-spin-col {
    height: 100px;
    position: relative;
    border: 1px solid #eee;
}

.ivu-spin-fix {
    top: 105px;
    background: rgba(0, 0, 0, .4);
}

.ivu-spin-main {
    .ivu-spin-text {
        i {
            font-size: 70px;
        }
    }
}

//list css
.list {
    position: relative;
    // white-space: nowrap;
    //display: table;
    background: url('../../../static/img/plant2.jpg') no-repeat center center;
    background-size: cover;
    //background: #eee;
    min-width: 830px;
    //background-color: transparent;
    display: -webkit-box;
    display: -moz-box;
    display: box;
    display: -webkit-flex;
    display: -moz-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    box-flex: 1;
    -webkit-flex: 1;
    -moz-flex: 1;
    -ms-flex: 1;
    flex: 1;
    min-height: 1px;
    //padding-top: 1em;
    padding-bottom: 1em;
    position: relative;
    .panegutter {
        -webkit-box-flex: 1;
        -moz-box-flex: 1;
        box-flex: 1;
        -webkit-flex: 1 0 20px;
        -moz-flex: 1 0 20px;
        -ms-flex: 1 0 20px;
        flex: 1 0 20px;
    }
    .listpart {
        -webkit-box-flex: 5500;
        -moz-box-flex: 5500;
        box-flex: 5500;
        -webkit-flex: 5500 1 605px;
        -moz-flex: 5500 1 605px;
        -ms-flex: 5500 1 605px;
        flex: 5500 1 605px;
        max-width: 1000px;
        min-width: 405px;
        border: 1px solid #ccc;
        padding: 0 1em;
        border-radius: 2px;
        background: white;
        overflow: auto;
        //height: auto;
        max-height: 480px;
        .table-hover {
            border-spacing: 0;
            border-collapse: collapse;
            width: 100%;
            max-width: 100%;
            margin-bottom: 20px;
            tr:hover {
                background-color: #f5f5f5;
            }
            tr:last-of-type td {
                border-bottom: 1px solid #ddd;
            }
            .row-focus td:not(:first-of-type) {
                border-top: 1px solid navy !important;
                border-bottom: 1px solid navy !important;
            }
            td {
                padding: 8px;
                line-height: 1.42857143;
                vertical-align: middle;
                border-top: 1px solid #ddd;
            }
            .listsort-drag {
                opacity: 0;
            }
            .drag-chosen {
                background-color: #ddd;
                .drag-handler {
                    opacity: 1;
                }
            }
            .drag-ghost {
                opacity: 1;
            }
            .drag-handler {
                width: 2em;
                opacity: 0;
                cursor: move;
                border-top: 1px solid transparent !important;
                border-bottom: 1px solid transparent !important;
            }
            .drag-handler:hover {
                opacity: 0.8;
            }
            .drag-handler-icon {
                background: url("../../../static/img/list-drag-handler.png");
                background-size: 16px 16px;
                display: inline-block;
                width: 16px;
                height: 16px;
                overflow: hidden;
                background-repeat: no-repeat;
                font-size: 0;
                line-height: 0;
                text-align: center;
            }
            .complete-icon {
                width: 2em;
                color: #ccc;
                font-weight: bold;
            }
            .complete-icon:hover {
                color: #333;
            }
            .item-detail {
                cursor: pointer;
                textarea {
                    width: 100%;
                    padding: 0;
                    background: transparent;
                    border: 0px solid transparent;
                    outline: 0px solid transparent;
                    height: 1.4285em;
                    overflow: hidden;
                    word-wrap: normal;
                    white-space: nowrap;
                    resize: none;
                    cursor: pointer;
                }
                .item-detail-div {
                    display: -webkit-box;
                    display: -moz-box;
                    display: box;
                    display: -webkit-flex;
                    display: -moz-flex;
                    display: -ms-flexbox;
                    display: flex;
                    cursor: pointer;
                }
                .item-other-info {
                    -webkit-box-align: center;
                    -moz-box-align: center;
                    box-align: center;
                    -webkit-align-items: center;
                    -moz-align-items: center;
                    -ms-align-items: center;
                    -o-align-items: center;
                    align-items: center;
                    -ms-flex-align: center;
                    display: -webkit-box;
                    display: -moz-box;
                    display: box;
                    display: -webkit-flex;
                    display: -moz-flex;
                    display: -ms-flexbox;
                    display: flex;
                    height: 100%;
                    cursor: pointer;
                }
                .item-tag {
                    background-color: #E1E2E4;
                    color: #898E95;
                    box-sizing: border-box;
                    display: block;
                    overflow: hidden;
                    text-overflow: ellipsis;
                    white-space: nowrap;
                    border-radius: 8.5px;
                    padding: 0 8px;
                    font-size: 12px;
                    margin-left: 5px;
                    max-width: 75px;
                    text-decoration: none;
                    height: 20px;
                    line-height: 18px;
                    border-collapse: separate;
                    border-spacing: 1px;
                }
                .item-due-day {
                    display: inline-block;
                    color: #676D76;
                    white-space: nowrap;
                    padding: 0 0.5em;
                    font-size: 12px;
                    display: flex;
                    height: 100%;
                }
            }
        }
    }
    .detailpart {
        -webkit-box-flex: 0;
        -moz-box-flex: 0;
        box-flex: 0;
        -webkit-flex: 0 0 365px;
        -moz-flex: 0 0 365px;
        -ms-flex: 0 0 365px;
        flex: 0 0 365px;
        -webkit-box-pack: start;
        -moz-box-pack: start;
        box-pack: start;
        -webkit-justify-content: flex-start;
        -moz-justify-content: flex-start;
        -ms-justify-content: flex-start;
        -o-justify-content: flex-start;
        justify-content: flex-start;
        -ms-flex-pack: start;
        margin-left: 15px;
        position: relative;
        top: 0px;
        //border: 1px solid #ccc;
        border-radius: 2px;
        display: none;
    }
    .button-small {
        text-align: center;
        white-space: nowrap;
        vertical-align: middle;
        -ms-touch-action: manipulation;
        touch-action: manipulation;
        cursor: pointer;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        background-image: none;
        border: 1px solid transparent;
        display: inline-block;
        margin-bottom: 0;
        font-weight: 400;
        padding: 5px 10px;
        font-size: 12px;
        line-height: 1.5;
        border-radius: 3px;
    }
    .button-default {
        color: #333;
        background-color: #fff;
        border-color: #ccc;
    }
    .button-default:focus {
        color: #333;
        background-color: #e6e6e6;
        border-color: #8c8c8c;
    }
    .button-default:hover {
        color: #333;
        background-color: #e6e6e6;
        border-color: #adadad;
    }
    .button-default:active {
        color: #333;
        background-color: #e6e6e6;
        border-color: #adadad;
    }
    .button-default:active:hover {
        color: #333;
        background-color: #d4d4d4;
        border-color: #8c8c8c;
    }
}