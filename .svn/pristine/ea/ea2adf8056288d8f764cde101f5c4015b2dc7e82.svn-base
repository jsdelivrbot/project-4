.btn-treeView, .btn-listView { height: 16px; width: 16px; display: block; text-indent: -9999px; }
.btn-treeView { background: url(images/list-tree-selector.gif) no-repeat 0 0; }
    a.btn-treeView:hover, .btn-treeView.selected { background: url(images/list-tree-selector.gif) no-repeat -16px 0; }
.btn-listView { background: url(images/list-tree-selector.gif) no-repeat -32px 0; }
    a.btn-listView:hover, .btn-listView.selected { background: url(images/list-tree-selector.gif) no-repeat -48px 0; }

.k-list-filter > .k-icon {top: 0px}

.k-task-summary .k-resize-handle {
  position: absolute;
  visibility: hidden;
  z-index: 2;
  height: auto;
}
.k-task-summary:hover .k-resize-handle{
  visibility: visible;
}
.k-task-summary .k-resize-handle:after {
  content: "";
  position: absolute;
  filter: alpha(opacity=50);
  opacity: .5;
}

.k-gantt-tasks {
  position: absolute;
  top: 0;
  left: 0;
}

.k-task-datanull {
    visibility: hidden;
}

.k-task-complete {
    margin:4px 0;
    background-color: #228B22 !important;
    background-image: none;
    border-radius: 0;
}

