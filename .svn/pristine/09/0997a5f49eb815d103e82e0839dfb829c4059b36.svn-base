html {
    overflow: hidden;
}

.boards-title {
    height: 32px;
    background: #F0F2F5;
    .boards-title-left {
        margin-left: 22px;
        line-height: 30px;
        .title-bread {
            position: relative;
            .file-png {
                margin-top: 3px;
                i {
                    font-size: 20px;
                    color: #000;
                }
            }
            .title-common {
                display: inline-block;
                margin-left: 10px;
                font-size: 20px;
            }
            .search-mes {
                font-size: 24px;
                transform: rotate(90deg);
                cursor: pointer;
                &:hover {
                    color: #58BFA8;
                }
            }
            .transform {
                transform: rotate(180deg);
                transition: transform .1s linear;
            }
            .search-mes-panel {
                position: absolute;
                top: 30px;
                left: 50%;
                width: 350px;
                background: #fff;
                transform: scale(0.7);
                z-index: 100;
                .search-mes-panel-top {
                    width: 348px;
                    height: 30px;
                    line-height: 30px;
                    text-align: right;
                    background: #F5F5F5;
                    border-bottom: 1px solid #ccc;
                    .x {
                        display: block;
                        width: 20px;
                        height: 20px;
                        line-height: 20px;
                        text-align: center;
                        margin: 3px;
                        border-radius: 2px;
                        cursor: pointer;
                        &:hover {
                            border: 1px solid #999999;
                            background: rgb(235, 235, 235);
                        }
                    }
                }
                .search-mes-input {
                    width: 300px;
                    height: 34px;
                    border: 1px solid #ccc;
                    border-radius: 3px;
                    margin: 10px 20px;
                    .search-msg-icon {
                        display: block;
                        width: 34px;
                        height: 32px;
                        line-height: 34px;
                        text-align: center;
                        background: #F5F5F5;
                        font-size: 22px;
                    }
                    .search-mes-input-bar {
                        width: 264px;
                        border: none;
                    }
                }
                .mes-panel-list {
                    margin: 10px 10px;
                    .panel-first-title {
                        .first-title-icon {
                            height: 16px;
                            line-height: 16px;
                            i {
                                font-size: 20px;
                            }
                            cursor: pointer;
                        }
                        .first-title-content {
                            display: block;
                            height: 16px;
                            line-height: 16px;
                            padding: 2px 22px;
                            margin-left: 20px;
                            background: url('../../../static/img/type-root-normal.png') no-repeat left center;
                        }
                        .second-title {
                            margin-left: 20px;
                            .dev-items-icon {
                                height: 16px;
                                line-height: 16px;
                                i {
                                    font-size: 20px;
                                }
                                cursor: pointer;
                            }
                            .second-title-content {
                                display: block;
                                height: 16px;
                                line-height: 16px;
                                padding: 2px 18px;
                                margin-left: 20px;
                                background: url('../../../static/img/type-space.png') no-repeat left center;
                            }
                            ul {
                                li {
                                    margin-left: 30px;
                                }
                            }
                        }
                    }
                }
            }
            .searchPanel {
                transform: scale(1);
                transition: all 5s linear;
            }
            .filter {
                position: relative;
                font-size: 18px;
                cursor: pointer;
                i.fa-filter:hover {
                    color: #58BFA8;
                }
                .filter-select {
                    transform: scale(0.7);
                    position: absolute;
                    top: 30px;
                    left: -100px;
                    width: 200px;
                    height: 234px;
                    background: #fff;
                    z-index: 10;
                    &.filterPanelShow {
                        transform: scale(1);
                        transition: transform 5s linear;
                    }
                    .filter-top {
                        width: 198px;
                        height: 30px;
                        line-height: 30px;
                        text-align: right;
                        background: #F5F5F5;
                        border-bottom: 1px solid #ccc;
                        .x {
                            display: block;
                            width: 20px;
                            height: 20px;
                            line-height: 20px;
                            text-align: center;
                            margin: 3px;
                            border-radius: 2px;
                            cursor: pointer;
                            font-size: 14px;
                            font-weight: 100;
                            &:hover {
                                border: 1px solid #999999;
                                background: rgb(235, 235, 235);
                            }
                        }
                        .filter-top-text {
                            padding-left: 10px;
                            font-size: 14px;
                        }
                    }
                    .filter-by {
                        //padding: 10px 5px;
                        .filter-by-item {
                            border-bottom: 1px solid #ccc;
                            padding-left: 10px;
                            margin: 0 5px;
                            &:last-child {
                                border-bottom: none;
                            }
                            a {
                                display: block;
                                margin: 0 5px;
                                padding: 5px 0 35px 40px;
                                font-size: 14px;
                                color: #1F2532;
                                &.followby {
                                    height: 24px;
                                    background: url('../../../static/img/followby.png') no-repeat left center;
                                }
                                &.assigned {
                                    height: 24px;
                                    background: url('../../../static/img/assignedby.png') no-repeat left center;
                                }
                                &.duedate {
                                    height: 24px;
                                    background: url('../../../static/img/duedate.png') no-repeat left center;
                                }
                                &.tags {
                                    height: 24px;
                                    background: url('../../../static/img/tags.png') no-repeat left center;
                                }
                                &.statusby {
                                    height: 24px;
                                    background: url('../../../static/img/statusby.png') no-repeat left center;
                                }
                            }
                        }
                    }
                }
            }
        }
    }
    .boardsBackend {
        position: absolute;
        top: 120px;
        left: 0;
        right: 0;
        bottom: 0;
        background: #000;
        opacity: 0.5;
        z-index: 9;
    }
}

.boards-title-right {
    margin-right: 22px;
    a {
        display: inline-block;
        background: #fff;
        color: #000;
        border-radius: 2px;
        margin-top: 2px;
        line-height: 24px;
        padding: 0 8px;
        margin-left: 5px;
    }
}

.statusbar {
    overflow-x: scroll;
    overflow-y: hidden;
    .status-title-common {
        padding: 5px 20px;
        font-size: 14px;
        display: inline-block;
        text-align: center;
        width: 220px;
    }
    &:hover {
        &::-webkit-scrollbar-track {
            -webkit-box-shadow: inset 0 0 8px rgba(0, 0, 0, 0.3);
            border-radius: 3px;
            background-color: #F5F5F5;
        }
        &::-webkit-scrollbar {
            width: 100%;
            height: 10px;
            background-color: #F5F5F5;
        }
        &::-webkit-scrollbar-thumb {
            background-color: #0DAFD5;
        }
    }
    &::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 8px rgba(0, 0, 0, 0.1);
        border-radius: 3px;
        background-color: transparent;
    }
    &::-webkit-scrollbar {
        width: 100%;
        height: 10px;
        background-color: transparent;
    }
    &::-webkit-scrollbar-thumb {
        background-color: transparent;
    }
}

.status-todo {
    background: #58BFA8;
    color: white;
}

.status-inprogress {
    background: #60A1ED;
    color: white;
}

.status-verify {
    background: #AFAFAF;
    color: white;
}

.status-done {
    background: #A257E3;
    color: white;
}

.wrapper {
    white-space: nowrap;
    height: 100%;
    max-height: 100%;
    min-height: 100%;
    overflow: hidden;
    &.hasLeftoffset {
        margin-left: 350px;
    }
    &.hasRightoffset {
        margin-right: 365px;
    }
    //background: linear-gradient(-45deg, rgb(0, 170, 255), rgb(36, 242, 225) 100%);
    &::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 8px rgba(0, 0, 0, 0.3);
        border-radius: 3px;
        background-color: #F5F5F5;
    }
    &::-webkit-scrollbar {
        width: 100%;
        height: 10px;
        background-color: #F5F5F5;
    }
    &::-webkit-scrollbar-thumb {
        background-color: #0DAFD5;
    }
    .container {
        max-width: 500px;
        width: 220px;
        display: inline-block;
        position: relative;
        display: inline-block;
        white-space: normal;
        height: 100%;
        vertical-align: top;
        &:nth-child(odd) {
            // background-color: rgba(0, 0, 0, 0.05);
        }
        &:nth-child(even) {
            //box-shadow: 1px 1px 1px #16C2C2;
        }
        .to-add-card {
            cursor: pointer;
            margin: 0 auto;
            max-width: 295px;
            position: static;
            .add-card-wrapper {
                height: 36px;
                box-sizing: border-box;
                margin: 0 16px;
                .add-card-box {
                    border: 1px dashed #999;
                    color: #cbcbcb;
                    font-size: 20px;
                    height: 100%;
                    text-align: center;
                    span {
                        padding-top: 0;
                        color: #cbcbcb;
                        font-size: 30px;
                    }
                }
                .form-box {
                    display: none;
                    height: 100%;
                    width: 100%;
                    background-color: #fff;
                    border: 1px solid #8ab98e;
                    z-index: 9999999;
                    .form-box-content {
                        height: 100%;
                        position: relative;
                        .form-input-wrapper {
                            height: 100%;
                            padding-right: 45px;
                            width: 100%;
                            input.newinput {
                                width: 99%;
                                background-color: #fff;
                                color: #333;
                                border-color: #aeaeae;
                                height: 2.13em;
                                text-indent: .33em;
                                line-height: 1.6em;
                                padding: 2px .3em;
                                border: none;
                                outline: none;
                            }
                        }
                        .form-input-btn {
                            float: right;
                            height: 100%;
                            position: absolute;
                            right: 0;
                            top: 0;
                            padding: 8px 15px;
                            background: #8ab98e;
                            text-shadow: none;
                            box-shadow: none;
                            outline: none;
                            transition: background-color 0.3s linear 0s;
                            span {
                                position: absolute;
                                top: 2%;
                                left: 32%;
                                font-size: 20px;
                                color: white;
                            }
                        }
                    }
                }
            }
        }
        .action-card {
            position: relative;
            // margin: 16px 16px 20px 25px;
            padding: 5px 10px;
            margin-top: 10px;
            width: 165px;
            height: 113px;
            background-color: #fff;
            border-radius: 3px;
            cursor: pointer;
            &.active {
                background: #DEEFE0;
            }
            &.unactive {
                background: #FFF;
            }
            &.bg-placeholder {
                background: #EDEDED;
                animation: none;
            }
            &:hover {
                box-shadow: 0 2px 2px rgba(0, 0, 0, 0.2);
            }
            .card-content {
                Button {
                    //border-radius:100%;
                    width: 85px;
                    height: 35px;
                    position: relative;
                    padding: 3px;
                    top: 0;
                    font-size: 14px;
                }
                .bg-content-button {
                    &.TS {
                        background: #333;
                    }
                    &.WN {
                        background: #58BFA8;
                    }
                    &.TJ {
                        background: #4A90E2;
                    }
                    &.JB {
                        background: #A257E3;
                    }
                    &.SW {
                        background: #1A8383;
                    }
                    &.PW {
                        background: #52640A;
                    }
                    &.MP {
                        background: #9D6C40;
                    }
                    &.PM {
                        background: #D8674B;
                    }
                    &.JR {
                        background: #9B211C;
                    }
                    &.PM {
                        background: #6F0D08;
                    }
                    &.KS {
                        background: #1AB917;
                    }
                    &.DS {
                        background: #584B98;
                    }
                    &.MM {
                        background: #1C98A5;
                    }
                    &.BB {
                        background: #838615;
                    }
                }
                .bg-content {
                    margin: 0 0 10px 10px;
                    width: 35px;
                    height: 35px;
                    line-height: 35px;
                    text-align: center;
                    border-radius: 100%;
                    color: #fff;
                    // background: #58bfa8;
                    font-size: 14px;
                    &.TS {
                        background: #333;
                    }
                    &.WN {
                        background: #58BFA8;
                    }
                    &.TJ {
                        background: #4A90E2;
                    }
                    &.JB {
                        background: #A257E3;
                    }
                    &.SW {
                        background: #1A8383;
                    }
                    &.PW {
                        background: #52640A;
                    }
                    &.MP {
                        background: #9D6C40;
                    }
                    &.PM {
                        background: #D8674B;
                    }
                    &.JC,&.JR {
                        background: #9B211C;
                    }
                    &.PM {
                        background: #6F0D08;
                    }
                    &.KS {
                        background: #1AB917;
                    }
                    &.DS {
                        background: #584B98;
                    }
                    &.MM {
                        background: #1C98A5;
                    }
                    &.BB {
                        background: #838615;
                    }
                }
                .card-msg {
                    width: auto;
                    font-size: 14px;
                    font-weight: normal;
                    line-height: 15px;
                    margin: 10px 0 20px 0;
                }
            }
            .card-meta {
                margin: 35px 0 5px 0;
            }
        }
        .empty-card {
            position: relative;
            // margin: 16px 16px 85px 25px;
            padding: 5px 10px;
            margin-top: 10px;
            width: 165px;
            height: 113px;
        }
    }
}

