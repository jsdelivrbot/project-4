 <script>
  var newTaskTemplate = '<div class="action-card selected" style="display:none" data-id="^dataID^" id="^ID^" status="1">';
	newTaskTemplate +=		'<div class="cf clearfix"><div class="my-avatar"></div>';
	newTaskTemplate +=		'<h4 class="action-card-title" title="^T^">^T^</h4>';
	newTaskTemplate +=	'</div>';
	newTaskTemplate +=	'<div class="action-card-meta clearfix">';
	newTaskTemplate +=		'<span class="id-badge right">ID ^ID^</span>';
	newTaskTemplate +=		'<div class="action-card-properties cf">';
	newTaskTemplate +=		'</div>';
	newTaskTemplate +=	'</div>';
	newTaskTemplate +=	'</div>';

	var testUsers = [
		{"key":"TJ","name":"Terry Johnson","color":"my-bg4"},
		{"key":"TZ","name":"Tim Zero","color":"my-bg1"},
		{"key":"JC","name":"Jimery Cook","color":"my-bg2"},
		{"key":"WW","name":"William Wall","color":"my-bg2"},
		{"key":"PH","name":"Phillips Hunt","color":"my-bg3"},
		{"key":"JZ","name":"John Zipper","color":"my-bg3"}
	];
	
	function wrapperclick(evt){
	
		if( evt.target.className.indexOf("inline-add-card") >= 0 || 
			evt.target.className.indexOf("k-textbox") >= 0 ||
			evt.target.className.indexOf("btn") >= 0 ||
			evt.target.className.indexOf("icon-")>=0){		
			return;
		}
		setNewTaskMode(false);
		setEditPanelVisible(false);
	}
	function setNewTaskMode(isNew){
		if( isNew){
			$(".inline-add-card__default-mode").hide();
			$(".inline-add-card__edit-mode").show();
			$(".k-textbox").focus();
		}
		else{
			$(".inline-add-card__default-mode").show();
			$(".inline-add-card__edit-mode").hide();
		}
	}
	function setEditPanelVisible(isShow){
		var windowWidth = $(window).outerWidth();
		if( isShow){
			$(".board-view").width(windowWidth -365);
			$(".title").width(windowWidth);
			$(".wrapper").width( windowWidth);

			$(".edit-panel").width( 365);
			$(".edit-panel").fadeIn("2000");
		}
		else{
			$(".board-view").css("width","100%");
			$(".title").css("width","100%");
			$(".wrapper").css("width","100%");
			$(".edit-panel").fadeOut();
		}
	}
	function NewTask(){
		setNewTaskMode(true);
	}

	var newTaskID = 100;
	function submitTask(){
		var taskTitle = $(".k-textbox").val();
		taskTitle.trim();
		if( taskTitle == "")
			return;
		
		$(".k-textbox").val("");
		//var taskID = "newTask_" + newTaskID;
		newTaskID += 1;
	/*	var insertNewTaskHtml = "<div class='selected' style='display:none;' " + "id='"+
					 taskID +"'> <img src=\"images/icon_newissue.gif\"/>" + newTaskID +"<span class=\"my-span-text\">"+ 
					taskTitle + "</span></div>";*/
		var insertNewTaskHtml = newTaskTemplate;
		insertNewTaskHtml = insertNewTaskHtml.replace("^T^",taskTitle).replace("^T^",taskTitle);
		insertNewTaskHtml = insertNewTaskHtml.replace("^ID^",newTaskID).replace("^ID^",newTaskID);
		insertNewTaskHtml = insertNewTaskHtml.replace("^dataID^",newTaskID);

		removeSelectedClass();
		$(".inline-add-card").after(insertNewTaskHtml);
		$("#"+ newTaskID).fadeIn("3000");
		$("#"+ newTaskID).bind("click",function(e){
			editItem(this);
			return false;
		});
	}
	function removeSelectedClass(){
		$("div",$(".wrapper")).each(function(){
			$(this).removeClass("selected");
		});
	}
	function hightlistAssigneeInComboxList(userKey,isAdd){
		$(".pp-comboboxlist",$(".edit-panel")).find("li").each(function(e){
				if($(".my-avatar",$(this)).text().trim() == userKey && isAdd)
					$(this).addClass("highlight");
				else
					$(this).removeClass("highlight");
			});
	}
	function GetFormattedCurrentDate() {
		var today = new Date();
		var dd = today.getDate();
		var mm = today.getMonth()+1; //January is 0!

		var yyyy = today.getFullYear();
		if(dd<10){
			dd='0'+dd;
		} 
		if(mm<10){
			mm='0'+mm;
		} 
		return mm +'/'+ dd+'/'+yyyy;
	}
	function editItem(obj){
		saveItemTitle();
		$("#descSave",$(".edit-panel")).trigger("click");
		$("#Comment",$(".edit-panel")).trigger("click");
		//set values to edit-panel
		var id = $(obj).find(".id-badge").html().replace("ID","");
		id.trim();
		$("#taskid",$(".edit-panel")).html(id);

		//title
		var title = $(obj).find(".action-card-title").html();
		$("#title",$(".edit-panel")).html(title);
		
		//assignee
		var userKey = $(obj).find(".my-avatar").text();
		userKey.trim();
		if( userKey != ""){
			setAssigneeInEditPanel(userKey);

			hightlistAssigneeInComboxList(userKey,true);
		}
		else{
			$(".sel-assignee_id",$(".edi-panel")).hide();
			$(".sel-assignee_name",$(".edit-panel")).hide();
			$(".pp-objectpicker",$(".sel-assignee_id")).hide();
			$(".icon-person",$(".edit-panel")).show();
			hightlistAssigneeInComboxList("",false);
		}
		//description
		var desc =	$(".hidden_Desc",$(obj)).text();
		if( desc != "")
		 	$(".po-markdown__text-block",$(".edit-panel")).html(desc);
		else
			$(".po-markdown__text-block",$(".edit-panel")).html("Edit description");
		
		//due date
		var dueDate = $(".icon-calendar",$(obj)).text();
		if( dueDate != ""){
			var calendar = $("#dueDate_calender").data("kendoCalendar");
       		calendar.value(dueDate);
			$(".sel-due-date",$(".edit-panel")).show();
			$(".pp-details-pane__attribute-value",$(".sel-due-date_info")).text(dueDate);
			$(".sel-due-date_info",$(".edit-panel")).show();
			$(".icon-calendar",$(".po-iconrow")).hide();
		}
		else{
			 var calendar = $("#dueDate_calender").data("kendoCalendar");
			 var currentDate = GetFormattedCurrentDate();
       		 calendar.value(currentDate);
			$("#dueDate").val(currentDate);
			$(".sel-due-date",$(".edit-panel")).hide();
			$(".sel-due-date_info",$(".edit-panel")).hide();
			$(".pp-datepicker",$(".edit-panel")).hide();
			$(".icon-calendar",$(".po-iconrow")).show();
		}

		//status 
		var status = $(obj).attr("status");
		var statusName = "To Do";
		switch( status){
			case "1":{
				statusName = "To Do";
				break;
			}
			case "2":{
				statusName = "In Progress";
				break;
			}
			case "3":{
				statusName = "QA Floater Verify";
				break;
			}
			case "4":{
				statusName = "Close Done";
				break;
			}
			default:
			break;
		}
		
		$("#statusName",$("#div_Status")).text(statusName);
		
		//comments
		var comments = $(".hidden_Comment",$(obj)).html();
		$(".my-comments-content",$(".edit-panel")).each(function(e){
			if($(this).attr("id") != "hidden_CommentTemplate")
				$(this).remove();
		});
		$(".po-newpost__container",$(".edit-panel")).after(comments);

		setCommentsCount();

		setEditPanelVisible(true);

		removeSelectedClass();
		$(obj).addClass("selected");
	}
	function saveItemTitle(){
		var title = $(".k-textbox",$(".edit-panel")).val();
		if( title == "")
			return;
		$("#title",$(".edit-panel")).html(title);
		$("#title",$(".edit-panel")).show();
		$(".pp-input-wrapper",$(".edit-panel")).hide();

		//update item's title in board view
		$(".action-card-title",$(".selected")).html(title);
		$(".k-textbox",$(".edit-panel")).val("");
	}
	function setCommentsCount(){
		var commentCount = 0;
		$(".my-comments-content",$(".edit-panel")).each(function(e){
			if( $(this).attr("id") != "hidden_CommentTemplate")
				commentCount += 1;
		});
		$("#CommentCount",$(".edit-panel")).text(commentCount + " Comments");
	}
	function setAssigneeInEditPanel(userKey){
		var newColor = "";
		var newName = "";
		for(var i = 0; i < testUsers.length ; ++i){
			if( testUsers[i].key == userKey){
				newColor = testUsers[i].color;
				newName = testUsers[i].name;
				break;
			}
		}
		$(".sel-assignee_id",$(".edit-panel")).show();
		$(".sel-assignee_name",$(".sel-assignee_id")).show();
		var $assigneeName = $(".my-avatar",$(".sel-assignee_name"));
	    $assigneeName[0].className = '';
		$assigneeName.addClass("my-avatar " + newColor);
		$assigneeName.text(userKey);
		
		$(".pp-details-pane__attribute-value",$(".sel-assignee_name")).text(newName);
		$(".pp-objectpicker",$(".sel-assignee_id")).hide();
		$(".icon-person",$(".edit-panel")).hide();
	}
