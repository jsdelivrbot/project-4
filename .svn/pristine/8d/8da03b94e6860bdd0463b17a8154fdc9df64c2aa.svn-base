.folder {
    .icons {
        width: 20px;
        height: 20px;
        line-height: 20px;
        cursor:pointer;
        .icon {
            display: inline-block;
            width: 15px;
            height: 15px;
            background-repeat: no-repeat;
            vertical-align: middle;
            &.folder {
                width: 20px;
                height: 20px;
                background-size: cover;
                transform: rotate(-90deg);
                background: url('../../../../static/img/s.png') no-repeat;
            }
            &.folder-open {
                width: 20px;
                height: 20px;
                background-size: cover;
                background: url('../../../../static/img/s.png') no-repeat;
            }
        }
    }
    .folder-toggle{
        .project-name{
           
        }
    }
    .node {
        margin-left: 20px;
    }
}
